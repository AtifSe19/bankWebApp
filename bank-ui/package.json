  "devDependencies": {
    "cpx": "^1.5.0",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "angular": "^1.8.3",
    "angular-resource": "^1.8.3",
    "bootstrap": "^5.3.1",
    "jquery": "^3.7.1"
  }
}
