{
  "name": "bank-ui",
  "version": "1.0.0",
  "description": "This is my bank app frontend",
  "main": "index.html",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "copy": "cpx node_modules/{jquery/dist,bootstrap/dist,angular,angular-resource}/**/* src/lib/ -u",
